{# templates/news/publish.html.twig #}

{% extends 'base.html.twig' %}

{% block title %}Publish News{% endblock %}

{% block body %}
    <h1>Publish News</h1>

    {{ form_start(form) }}
        {{ form_row(form.title) }}
        {{ form_row(form.description) }}
        {{ form_row(form.category) }}
        <button type="submit" class="btn btn-primary">Publish</button>
    {{ form_end(form) }}
{% endblock %}
